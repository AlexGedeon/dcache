<project name="server-deb-build">

    <!--
          Debian Targets
    -->

    <target name="server.deb" depends="server.rpm" description="Build dcache-server-xxx.deb">
        <exec dir="${rpmRpmDir}/noarch" executable="fakeroot" failonerror="true">
            <arg line="alien -k dcache-server-${version}-${patch}.noarch.rpm" />
        </exec>
    </target>

</project>
