# Implements the stage PEP logic.

# Disables stage PEP in this domain
define env stage-disable-pep.exe end
  set env stageConfigurationFilePath ""
end

# Decide whether to enable the stage PEP
onerror continue
eval ${stagePolicyEnforcementPoint} ${0} !=
exec env stage-disable-pep.exe -ifok
onerror shutdown

