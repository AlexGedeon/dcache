<?xml version="1.0"?>
<digester-rules>
  <pattern value="tables">
    <object-create-rule classname="org.dcache.web.TableList" />
    <set-properties-rule />
    
    <pattern value="table">
      <object-create-rule classname="org.dcache.web.TableElem"/>
      <set-properties-rule/>
      <set-next-rule methodname="addTable" />                      <!-- Method of org.dcache.web.TableList class -->
      
      <pattern value="create">
        <object-create-rule classname="org.dcache.web.QueryElem" />
        <set-properties-rule/>
        <set-next-rule methodname="setCreateQuery" />              <!-- Method of org.dcache.web.TableElem class -->
        <call-method-rule methodname="setQuery" paramcount="0" />  <!-- Method of org.dcache.web.QueryElem class -->
      </pattern>
      
      <pattern value="update">
        <object-create-rule classname="org.dcache.web.QueryElem" />
        <set-properties-rule/>
        <set-next-rule methodname="setUpdateQuery" />              <!-- Method of org.dcache.web.TableElem class -->
        <call-method-rule pattern="query" methodname="addQuery" paramcount="1" />
        <call-param-rule pattern="query" paramnumber="0"/>
      </pattern>

    </pattern>  <!-- table -->
    
    <call-method-rule pattern="execute" methodname="execute" paramcount="1" />
    <call-param-rule  pattern="execute" paramnumber="0"/>
  </pattern>  <!-- tables -->
</digester-rules>
