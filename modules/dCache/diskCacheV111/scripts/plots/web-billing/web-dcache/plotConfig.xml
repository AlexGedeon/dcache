<?xml version="1.0" encoding="UTF-8"?>
<plots>

	<plot id="billing.brd.day">
		<title>Bytes Read</title>
		<datasource table="dc_rd_hourly">
<!--			<![CDATA[datestamp >= current_date and datestamp < current_date+interval '1 day' ORDER BY datestamp]]> -->
			datestamp between 'now'::timestamp(0)-interval '1 day' and 'now'::timestamp(0) ORDER BY datestamp
		</datasource>
		<datasource table="en_rd_hourly">
<!--			<![CDATA[datestamp >= current_date and datestamp < current_date+interval '1 day' ORDER BY datestamp]]> -->
			datestamp between 'now'::timestamp(0)-interval '1 day' and 'now'::timestamp(0) ORDER BY datestamp
		</datasource>
		<gnusetup>
			<c>set xdata time</c>
			<c>set timefmt '%Y-%m-%d %H'</c>
			<c>set format x '%H:%M'</c>
			<c>set xlabel 'Time (HH:MM)'</c>
			<c>set ylabel 'Bytes'</c>
			<!-- <c>set xrange ['00:00':'23:59']</c> -->
			<dataset title="dCache" src="dc_rd_hourly">
				using {datestamp}:(${transfersize}) with boxes lt 2
			</dataset>
			<dataset title="Enstore" src="en_rd_hourly">
				using {datestamp}:(-${fullsize}) with boxes lt 1
			</dataset>
		</gnusetup>
	</plot>

	<plot id="billing.brd.week">
		<title>Bytes Read</title>
